<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pasoom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminLogin"></activity>
        <activity android:name=".AdminMain"></activity>
        <activity android:name=".UserLogin"></activity>
        <activity android:name=".UserMain"></activity>
        <activity android:name=".AdminRegister"></activity>
        <activity android:name=".UserRegister"></activity>
        <service android:name=".CamOver"></service>
        <service android:name=".alert"></service>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>